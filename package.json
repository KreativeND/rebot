{
  "name": "refactor",
  "displayName": "rebot",
  "publisher": "rebot",
  "description": "Refactor your code within VS Code with the power of genAI.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rebot.startRebot",
        "title": "start rebot",
        "category": "rebot"
      },
      {
        "command": "rebot.showFileTree",
        "title": "show file tree",
        "category": "rebot"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "webview",
          "title": "REBOT",
          "icon": "assets/icons/logo.svg"
        }
      ]
    },
    "views": {
      "webview": [
        {
          "type": "webview",
          "id": "left-panel-webview",
          "name": "REBOT",
          "icon": "assets/icons/logo.svg"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dagre": "^0.8.5"
  }
}
